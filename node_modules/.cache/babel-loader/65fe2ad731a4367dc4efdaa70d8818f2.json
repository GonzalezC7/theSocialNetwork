{"ast":null,"code":"export default {\n  data() {\n    return {\n      firstname: \"\",\n      lastname: \"\",\n      email: \"\",\n      age: \"\",\n      occupation: \"\",\n      result: null,\n      token: \"\"\n    };\n  },\n\n  methods: {\n    async user() {\n      const options = {\n        method: \"PUT\",\n        headers: {\n          \"Content-Type\": \"application/json\",\n          Authorization: `Bearer ${token}`\n        },\n        body: JSON.stringify({\n          firstname: this.firstname,\n          lastname: this.lastname,\n          email: this.email,\n          age: this.age,\n          occupation: this.occupation\n        })\n      };\n      const response = await fetch(\"https://social-network-api.osc-fr1.scalingo.io/fakebook-reseau/user\", options);\n      const data = await response.json();\n      this.result = data.success;\n\n      if (data.success === true) {\n        this.token = data.token;\n        localStorage.setItem(\"token\", data.token);\n      }\n    },\n\n    goHome() {\n      this.$router.push({\n        name: \"Welcome\"\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAsFA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,SAAS,EAAE,EADN;MAELC,QAAQ,EAAE,EAFL;MAGLC,KAAK,EAAE,EAHF;MAILC,GAAG,EAAE,EAJA;MAKLC,UAAU,EAAE,EALP;MAMLC,MAAM,EAAE,IANH;MAOLC,KAAK,EAAE;IAPF,CAAP;EASD,CAXY;;EAabC,OAAO,EAAE;IACP,MAAMC,IAAN,GAAa;MACX,MAAMC,OAAM,GAAI;QACdC,MAAM,EAAE,KADM;QAEdC,OAAO,EAAE;UACP,gBAAgB,kBADT;UAEPC,aAAa,EAAG,UAASN,KAAM;QAFxB,CAFK;QAMdO,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAe;UACnBf,SAAS,EAAE,KAAKA,SADG;UAEnBC,QAAQ,EAAE,KAAKA,QAFI;UAGnBC,KAAK,EAAE,KAAKA,KAHO;UAInBC,GAAG,EAAE,KAAKA,GAJS;UAKnBC,UAAU,EAAE,KAAKA;QALE,CAAf;MANQ,CAAhB;MAeA,MAAMY,QAAO,GAAI,MAAMC,KAAK,CAC1B,qEAD0B,EAE1BR,OAF0B,CAA5B;MAKA,MAAMV,IAAG,GAAI,MAAMiB,QAAQ,CAACE,IAAT,EAAnB;MAEA,KAAKb,MAAL,GAAcN,IAAI,CAACoB,OAAnB;;MACA,IAAIpB,IAAI,CAACoB,OAAL,KAAiB,IAArB,EAA2B;QACzB,KAAKb,KAAL,GAAaP,IAAI,CAACO,KAAlB;QACAc,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BtB,IAAI,CAACO,KAAnC;MACF;IACD,CA7BM;;