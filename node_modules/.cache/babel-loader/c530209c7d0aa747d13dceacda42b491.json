{"ast":null,"code":"export default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      firstname: \"\",\n      lastname: \"\",\n      result: null,\n      token: \"\"\n    };\n  },\n\n  methods: {\n    async register() {\n      const options = {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: JSON.stringify({\n          firstname: this.firstname,\n          lastname: this.lastname,\n          email: this.email,\n          age: this.age,\n          occupation: this.occupation\n        })\n      };\n      const response = await fetch(\"https://social-network-api.osc-fr1.scalingo.io/fakebook-reseau/register\", options);\n      const data = await response.json();\n      this.result = data.success;\n\n      if (data.success === true) {\n        this.token = data.token;\n        localStorage.setItem(\"token\", data.token);\n      }\n    },\n\n    goHome() {\n      this.$router.push({\n        name: \"Welcome\"\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwEA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EADF;MAELC,QAAQ,EAAE,EAFL;MAGLC,SAAS,EAAE,EAHN;MAILC,QAAQ,EAAE,EAJL;MAKLC,MAAM,EAAE,IALH;MAMLC,KAAK,EAAE;IANF,CAAP;EAQD,CAVY;;EAYbC,OAAO,EAAE;IACP,MAAMC,QAAN,GAAiB;MACf,MAAMC,OAAM,GAAI;QACdC,MAAM,EAAE,MADM;QAEdC,OAAO,EAAE;UACP,gBAAgB;QADT,CAFK;QAKdC,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAe;UACnBX,SAAS,EAAE,KAAKA,SADG;UAEnBC,QAAQ,EAAE,KAAKA,QAFI;UAGnBH,KAAK,EAAE,KAAKA,KAHO;UAInBc,GAAG,EAAE,KAAKA,GAJS;UAKnBC,UAAU,EAAE,KAAKA;QALE,CAAf;MALQ,CAAhB;MAcA,MAAMC,QAAO,GAAI,MAAMC,KAAK,CAC1B,yEAD0B,EAE1BT,OAF0B,CAA5B;MAKA,MAAMT,IAAG,GAAI,MAAMiB,QAAQ,CAACE,IAAT,EAAnB;MAEA,KAAKd,MAAL,GAAcL,IAAI,CAACoB,OAAnB;;MACA,IAAIpB,IAAI,CAACoB,OAAL,KAAiB,IAArB,EAA2B;QACzB,KAAKd,KAAL,GAAaN,IAAI,CAACM,KAAlB;QACAe,YAAY,CAACC,OAAb,CAAqB,OAArB,EAA8BtB,IAAI,CAACM,KAAnC;MACF;IACD,CA5BM;;IA6BPiB,MAAM,GAAG;MACP,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAAEC,IAAI,EAAE;MAAR,CAAlB;IACD;;EA/BM;AAZI,CAAf","names":[